journal = {Proceedings of the National Academy of Sciences},
keywords = {Gene Expression Profiling,Humans,Immunohistochemistry,Male,Nucleic Acid Hybridization,Prostatic Neoplasms,Prostatic Neoplasms: genetics,Prostatic Neoplasms: pathology},
number = {2},
pages = {615--620},
pmid = {14722351},
title = {{In silico dissection of cell-type-associated patterns of gene expression in prostate cancer}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=327196{\&}tool=pmcentrez{\&}rendertype=abstract},
