file = {:home/greg/Dropbox/work/research/2016/10-October/litrev/papers/Abbas2009.PDF:PDF},
isbn = {1932-6203 (Electronic)},
issn = {19326203},
journal = {PLoS ONE},
number = {7},
pmid = {19568420},
title = {{Deconvolution of blood microarray data identifies cellular activation patterns in systemic lupus erythematosus}},
volume = {4},
year = {2009}
}
